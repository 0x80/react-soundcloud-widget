{
  "name": "react-soundcloud",
  "version": "0.0.1",
  "description": "minimal soundcloud player",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/compedit/react-soundcloud.git"
  },
  "keywords": [
    "soundcloud",
    "player",
    "widget",
    "react"
  ],
  "author": "troy betz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/compedit/react-soundcloud/issues"
  },
  "homepage": "https://github.com/compedit/react-soundcloud",
  "dependencies": {
    "react": "^0.11.1"
  },
  "devDependencies": {
    "browserify": "^5.9.1",
    "http-server": "^0.6.1",
    "jest-cli": "^0.1.18",
    "react-tools": "^0.11.0",
    "reactify": "^0.14.0",
    "watchify": "^1.0.1"
  },
  "scripts": {
    "test": "jest",
    "start": "npm run serve & npm run watch-js",
    "serve": "./node_modules/.bin/http-server ./example -s",
    "watch-js": "./node_modules/.bin/watchify ./example/app.js -o ./example/bundle.js"
  },
  "browserify": {
    "transform": [
      "reactify"
    ]
  },
  "jest": {
    "rootDir": "./__tests__",
    "scriptPreprocessor": "../jest/preprocessor.js",
    "unmockedModulePathPatterns": [
      "react"
    ]
  }
}
